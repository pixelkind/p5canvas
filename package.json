{
  "name": "p5canvas",
  "displayName": "p5canvas",
  "description": "Live preview for p5js coding",
  "version": "1.5.1",
  "publisher": "garrit",
  "engines": {
    "vscode": "^1.32.0"
  },
  "icon": "images/p5canvas-icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.saveAsPNG",
        "title": "Saves the current p5canvas as PNG"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^12",
    "@types/vscode": "1.32.0",
    "mocha": "^7.1.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "jshint": "^2.11.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelkind/p5canvas"
  },
  "bugs": {
    "url": "https://github.com/pixelkind/p5canvas/issues"
  },
  "contributors": [
    {
      "name": "Garrit Schaap",
      "url": "https://github.com/pixelkind"
    },
    {
      "name": "Davide Becker",
      "url": "https://github.com/DavideBecker"
    },
    {
      "name": "Silvia Hundegger",
      "url": "https://github.com/silvia77"
    }
  ],
  "author": {
    "name": "Garrit Schaap",
    "url": "http://www.garrit.io"
  }
}
