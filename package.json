{
  "name": "p5canvas",
  "displayName": "p5canvas",
  "description": "Live preview for p5js coding",
  "version": "1.1.2",
  "publisher": "garrit",
  "engines": {
    "vscode": "^1.26.0"
  },
  "icon": "images/p5canvas-icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.saveAsPNG",
        "title": "Saves the current p5canvas as PNG"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^6.14.3",
    "mocha": "^5.2.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.30"
  },
  "dependencies": {
    "jshint": "^2.10.1",
    "ws": "^6.1.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelkind/p5canvas"
  },
  "bugs": {
    "url": "https://github.com/pixelkind/p5canvas/issues"
  },
  "contributors": [
    {
      "name": "Garrit Schaap",
      "url": "https://github.com/pixelkind"
    },
    {
      "name": "Davide Becker",
      "url": "https://github.com/DavideBecker"
    },
    {
      "name": "Silvia Hundegger",
      "url": "https://github.com/silvia77"
    }
  ],
  "author": {
    "name": "Garrit Schaap",
    "url": "http://www.garrit.io"
  }
}
