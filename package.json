{
  "name": "p5canvas",
  "displayName": "p5canvas",
  "description": "Live preview for p5js coding",
  "version": "1.7.0",
  "publisher": "garrit",
  "engines": {
    "vscode": "^1.41.0"
  },
  "icon": "images/p5canvas-icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "p5",
    "p5canvas",
    "auto-reload",
    "creative coding",
    "live coding"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.saveAsPNG",
        "title": "Saves the current p5canvas as PNG"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.19.7",
    "@types/vscode": "1.41.0",
    "mocha": "^7.2.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "jshint": "^2.12.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelkind/p5canvas"
  },
  "bugs": {
    "url": "https://github.com/pixelkind/p5canvas/issues"
  },
  "contributors": [
